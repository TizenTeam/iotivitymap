	group {
		name: "poi-marker-image";

		images {
			image: "../res/images/icon_map_location_02.png" COMP;
			image: "../res/images/icon_map_location_02_ef.png" COMP;
			image: "../res/images/icon_map_location_shadow.png" COMP;
			image: "../res/images/icon_map_location_01.png" COMP;
		}

		parts {
			part {
				name: "bg";
				type: RECT;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0 0;
					fixed: 1 1;
					min: 46 56;
					max: 46 56;
					visible: 1;
					rel1 { offset: -46/2 -56+8; }
					rel2 { offset: -46/2 -56+8; }
					color: 0 0 0 0;
				}
			}
			part {
				name: "shadow";
				type: IMAGE;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0 0;
					fixed: 1 1;
					min: 46 56;
					max: 46 56;
					visible: 1;
					image.normal: "../res/images/icon_map_location_shadow.png";
					rel1.to: "bg";
					rel2.to: "bg";
				}
			}
			part {
				name: "marker";
				type: IMAGE;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0 0;
					fixed: 1 1;
					color_class: "AO004";
					min: 46 56;
					max: 46 56;
					visible: 1;
					image.normal: "../res/images/icon_map_location_02.png";
					rel1.to: "bg";
					rel2.to: "bg";
				}
				description {
					state: "press" 0.0;
					inherit: "default" 0.0;
					color_class: "AO004P";
				}
			}
			part {
				name: "over-marker";
				type: IMAGE;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0 0;
					fixed: 1 1;
					min: 46 56;
					max: 46 56;
					rel1.to: "marker";
					rel2.to: "marker";
					visible: 1;
					image.normal: "../res/images/icon_map_location_01.png";
				}
			}
			part {
				name: "marker_stroke";
				type: IMAGE;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0 0;
					fixed: 1 1;
					min: 46 56;
					max: 46 56;
					rel1.to: "marker";
					rel2.to: "marker";
					visible: 1;
					image.normal: "../res/images/icon_map_location_02_ef.png";
				}
			}
			part {
				name: "marker-event";
				type: RECT;
				mouse_events: 1;
				scale: 1;
				description {
					state: "default" 0.0;
					color: 0 0 0 0;
					rel1.to: "bg";
					rel2.to: "bg";
				}
			}
		}
		programs {
			program {
				name: "marker_clicked";
				signal: "mouse,clicked,1";
				source: "marker-event";
				script {
					set_state(PART:"marker", "press", 0.0);
					run_program(PROGRAM:"touch_sound");
					emit("marker,clicked", "");
				}
			}
			program {
				name: "marker_set_selected";
				signal: "set_selected";
				source: "marker-event";
				script {
					set_state(PART:"marker", "press", 0.0);
				}
			}
			program {
				name: "marker_set_unselected";
				signal: "set_unselected";
				source: "marker-event";
				script {
					set_state(PART:"marker", "default", 0.0);
				}
			}
			program {
				name: "touch_sound";
				action: RUN_PLUGIN "touch_sound";
			}
		}
	}

